// Sample from https://districtdatalabs.silvrback.com/how-to-develop-quality-python-code
// Two types: dir and file.
// Directories must contain "name" string, followed by contents.

{
	"dir": 
	{
		// Root directory of project.
		"name": "myproject",
		"file": "README",
		"file": "LICENSE",
		"file": "requirements.txt",
		"file": "setup.py",
		"dir": 
		{
			// Main script.
			"name": "bin"
		},
		"dir": 
		{
			// Sphinx doc files.
			"name": "docs"
		},
		"dir": 
		{
			// Data and other sundry stuff.
			"name": "fixtures"
		},
		"dir": 
		{
			// Source directory.
			"name": "foo",
			"file": "__init__.py"
		},
		"dir": 
		{
			// Test files.
			"name": "tests",
			"file": "__init__.py"
		},
	}

}
